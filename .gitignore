# Ignore node_modules directory
node_modules/

# Ignore build and distribution directories
dist/
build/

# Ignore environment variables file
.env
.env.local

# Ignore log files
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Ignore operating system specific files
.DS_Store  # macOS
Thumbs.db  # Windows

# Ignore dependency lock files
package-lock.json
yarn.lock

# Ignore temporary files created by text editors and IDEs
.vscode/       # Visual Studio Code
.idea/         # JetBrains IDEs
*.swp          # Vim swap files
*.swo          # Vim temporary files
*.tmp          # General temporary files

# Ignore coverage reports
coverage/

# Ignore compiled Sass/SCSS files if the source files are tracked
*.css.map
*.scssc
*.sassc

# Ignore compressed files
*.zip
*.tar.gz

# Ignore files related to HTTP caches
.cache/
